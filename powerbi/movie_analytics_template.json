          }
        ]
      },
      {
        "name": "era_analytics",
        "source": "gold.era_analytics",
        "columns": [
          {
            "name": "movie_era",
            "dataType": "string",
            "displayName": "Movie Era"
          },
          {
            "name": "total_movies",
            "dataType": "int64",
            "displayName": "Total Movies"
          },
          {
            "name": "avg_rating",
            "dataType": "double",
            "displayName": "Average Rating",
            "formatString": "0.0"
          },
          {
            "name": "avg_budget",
            "dataType": "double",
            "displayName": "Average Budget",
            "formatString": "$#,##0"
          },
          {
            "name": "avg_box_office",
            "dataType": "double",
            "displayName": "Average Box Office",
            "formatString": "$#,##0"
          },
          {
            "name": "profitability_rate",
            "dataType": "double",
            "displayName": "Profitability Rate",
            "formatString": "0.0%"
          }
        ]
      }
    ],
    "relationships": [
      {
        "name": "GenreRelationship",
        "fromTable": "movie_performance_summary",
        "fromColumn": "genre",
        "toTable": "genre_analytics",
        "toColumn": "genre",
        "crossFilterDirection": "both"
      },
      {
        "name": "EraRelationship",
        "fromTable": "movie_performance_summary",
        "fromColumn": "movie_era",
        "toTable": "era_analytics",
        "toColumn": "movie_era",
        "crossFilterDirection": "both"
      }
    ],
    "measures": [
      {
        "name": "Total Revenue",
        "expression": "SUM('movie_performance_summary'[box_office])",
        "formatString": "$#,##0,,\"M\"",
        "displayFolder": "Financial KPIs"
      },
      {
        "name": "Total Profit",
        "expression": "SUM('movie_performance_summary'[profit])",
        "formatString": "$#,##0,,\"M\"",
        "displayFolder": "Financial KPIs"
      },
      {
        "name": "Average ROI",
        "expression": "AVERAGE('movie_performance_summary'[roi])",
        "formatString": "0.0\"%\"",
        "displayFolder": "Financial KPIs"
      },
      {
        "name": "Blockbuster Rate",
        "expression": "DIVIDE(COUNTROWS(FILTER('movie_performance_summary', 'movie_performance_summary'[is_blockbuster] = TRUE)), COUNTROWS('movie_performance_summary')) * 100",
        "formatString": "0.0\"%\"",
        "displayFolder": "Performance KPIs"
      },
      {
        "name": "Profitability Rate",
        "expression": "DIVIDE(COUNTROWS(FILTER('movie_performance_summary', 'movie_performance_summary'[is_profitable] = TRUE)), COUNTROWS('movie_performance_summary')) * 100",
        "formatString": "0.0\"%\"",
        "displayFolder": "Performance KPIs"
      },
      {
        "name": "Average Performance Score",
        "expression": "AVERAGE('movie_performance_summary'[performance_score])",
        "formatString": "0.0",
        "displayFolder": "Performance KPIs"
      },
      {
        "name": "Movie Count",
        "expression": "COUNTROWS('movie_performance_summary')",
        "formatString": "#,##0",
        "displayFolder": "Basic Metrics"
      },
      {
        "name": "Average Rating",
        "expression": "AVERAGE('movie_performance_summary'[rating])",
        "formatString": "0.0",
        "displayFolder": "Basic Metrics"
      },
      {
        "name": "Top Performer Count",
        "expression": "COUNTROWS(FILTER('movie_performance_summary', 'movie_performance_summary'[performance_score] > 8))",
        "formatString": "#,##0",
        "displayFolder": "Performance KPIs"
      },
      {
        "name": "High Budget Movies",
        "expression": "COUNTROWS(FILTER('movie_performance_summary', 'movie_performance_summary'[budget] > 100000000))",
        "formatString": "#,##0",
        "displayFolder": "Budget Analysis"
      }
    ],
    "calculatedColumns": [
      {
        "tableName": "movie_performance_summary",
        "name": "Budget Category",
        "expression": "SWITCH(TRUE(), 'movie_performance_summary'[budget] >= 100000000, \"Big Budget (>$100M)\", 'movie_performance_summary'[budget] >= 50000000, \"Medium Budget ($50M-$100M)\", 'movie_performance_summary'[budget] >= 10000000, \"Low Budget ($10M-$50M)\", \"Micro Budget (<$10M)\")",
        "dataType": "string"
      },
      {
        "tableName": "movie_performance_summary",
        "name": "Decade",
        "expression": "INT('movie_performance_summary'[year] / 10) * 10",
        "dataType": "int64"
      },
      {
        "tableName": "movie_performance_summary",
        "name": "Profit Margin",
        "expression": "IF('movie_performance_summary'[box_office] > 0, 'movie_performance_summary'[profit] / 'movie_performance_summary'[box_office], 0)",
        "dataType": "double",
        "formatString": "0.0%"
      },
      {
        "tableName": "movie_performance_summary",
        "name": "Success Level",
        "expression": "SWITCH(TRUE(), 'movie_performance_summary'[performance_score] >= 9, \"Exceptional\", 'movie_performance_summary'[performance_score] >= 8, \"Excellent\", 'movie_performance_summary'[performance_score] >= 7, \"Good\", 'movie_performance_summary'[performance_score] >= 6, \"Average\", \"Below Average\")",
        "dataType": "string"
      }
    ]
  },
